<%= render 'shared/title', title: @query%>

<%= render 'shared/tasks_nav', year: @year %>

<% if current_user.accountant? %>
  <%= render 'session', raw_type: @raw_type, tasks: @tasks %><br>
<% else %>
  <%= render 'session', raw_type: @raw_type, tasks: @officer_tasks %><br>
  <%= render 'session', raw_type: @raw_type, tasks: @leader_tasks %><br>
  <%= render 'session', raw_type: @raw_type, tasks: @manager_tasks %>
<% end %>